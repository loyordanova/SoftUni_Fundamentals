operator = input()
num1 = int(input())
num2 = int(input())


def calculate(a, b, c):

    if a == 'multiply':
        return int(b * c)
    elif a == 'divide':
        return int(b / c)
    elif a == 'add':
        return int(b + c)
    elif a == 'subtract':
        return int(b - c)


print(calculate(operator, num1, num2))
